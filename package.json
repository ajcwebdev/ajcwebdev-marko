    "preview": "wrangler dev"
  },
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@marko/build": "^4.2.3",
    "@marko/compiler": "^5.19.1",
    "@marko/serve": "^4.2.3",
    "@marko/vite": "^2.2.6",
    "cross-env": "^7.0.3",
    "marko": "^5.19.0",
    "rimraf": "^3.0.2",
    "serve-static": "^1.14.2",
    "undici": "^4.13.0",
    "url-router": "^13.0.0",
    "vary": "^1.1.2",
    "vite": "^2.7.13"
  },
  "type": "module"
}
